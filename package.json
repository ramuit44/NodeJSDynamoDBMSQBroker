{
  "name": "ccits-crud-service",
  "version": "1.0.0",
  "description": "CCITS Integration layer following a Decoupled Invocation pattern with AWS Lambda and serverless",
  "main": "handler.js",
  "scripts": {
    "invoke:local": "serverless invoke local --function",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint:fix": "npm run lint -- --fix",
    "flowtyped": "flow-typed install",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:all": "npm run lint && npm run typecheck && npm run test",
    "typecheck": "flow",
    "deploy": "sls deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ramuit44@bitbucket.org/karteldev/ccits-integration.git"
  },
  "keywords": [
    "Lambda",
    "DynamoDB",
    "API",
    "Gateway"
  ],
  "author": "Sriram Puvvada",
  "license": "MIT",
  "homepage": "https://bitbucket.org/karteldev/ccits-integration#readme",
  "devDependencies": {
    "aws-sdk": "^2.196.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-promise": "^3.5.0",
    "flow-bin": "^0.54.1",
    "flow-typed": "^2.1.5",
    "jest": "^21.0.2",
    "serverless": "^1.26.0",
    "serverless-webpack": "3.0.0-rc.2",
    "webpack": "3.3.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.2.1"
  }
}
